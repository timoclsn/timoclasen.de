{
  "name": "timoclasen.de",
  "description": "My personal website.",
  "version": "2.22.0",
  "private": true,
  "author": {
    "name": "Timo Clasen",
    "email": "timo@timoclasen.de",
    "url": "https://timoclasen.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timoclsn/timoclasen.de.git"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev -p 3000",
    "inspect": "NODE_OPTIONS='--inspect' next dev -p 3000",
    "build": "next build",
    "generate-rss": "node ./scripts/generate-rss.mjs",
    "generate-sitemap": "node ./scripts/generate-sitemap.mjs",
    "generate-podcasts": "node ./scripts/podcasts/index.mjs",
    "postbuild": "npm-run-all --parallel generate-rss generate-sitemap",
    "analyze": "ANALYZE=true next build",
    "start": "next start -p 3000",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint --ignore-path .gitignore",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc",
    "prettier": "prettier --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "format": "pnpm prettier -- --write",
    "check-format": "pnpm prettier -- --list-different",
    "validate": "npm-run-all --parallel typecheck check-format lint"
  },
  "dependencies": {
    "@mapbox/polyline": "1.2.1",
    "@prisma/client": "5.5.2",
    "@radix-ui/react-tabs": "1.0.4",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.10",
    "@tanstack/react-query": "5.4.3",
    "@trpc/client": "next",
    "@trpc/next": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "@vercel/og": "0.5.20",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.0",
    "js-confetti": "0.11.0",
    "match-sorter": "6.3.1",
    "next": "14.0.1",
    "plaiceholder": "3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "2.0.10",
    "react-hot-toast": "2.4.1",
    "react-markdown": "9.0.0",
    "react-syntax-highlighter": "15.5.0",
    "reading-time": "1.5.0",
    "remark": "15.0.1",
    "remark-html": "16.0.1",
    "remark-strip-html": "1.0.2",
    "simplify-geojson": "1.0.5",
    "strip-markdown": "6.0.0",
    "tailwindcss": "3.3.5",
    "ws": "8.14.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@types/geojson": "7946.0.12",
    "@types/jest": "29.5.7",
    "@types/mapbox__polyline": "1.0.4",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@types/react-syntax-highlighter": "15.5.9",
    "@types/testing-library__jest-dom": "5.14.9",
    "@types/ws": "8.5.8",
    "autoprefixer": "10.4.16",
    "cypress": "13.4.0",
    "dotenv": "16.3.1",
    "eslint": "8.52.0",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-prettier": "5.0.1",
    "fast-xml-parser": "4.3.2",
    "feed": "4.2.2",
    "globby": "13.2.2",
    "he": "1.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "prisma": "5.5.2",
    "sharp": "0.32.6",
    "typescript": "5.2.2"
  }
}
