{
  "name": "timoclasen.de",
  "description": "My personal website.",
  "version": "2.22.0",
  "private": true,
  "author": {
    "name": "Timo Clasen",
    "email": "timo@timoclasen.de",
    "url": "https://timoclasen.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timoclsn/timoclasen.de.git"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "inspect": "NODE_OPTIONS='--inspect' next dev -p 3000",
    "build": "next build",
    "generate-rss": "node ./scripts/generate-rss.mjs",
    "generate-sitemap": "node ./scripts/generate-sitemap.mjs",
    "generate-podcasts": "node ./scripts/podcasts/index.mjs",
    "postbuild": "npm-run-all --parallel generate-rss generate-sitemap",
    "analyze": "ANALYZE=true next build",
    "start": "next start -p 3000",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint --ignore-path .gitignore",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc",
    "prettier": "prettier --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel typecheck check-format lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.1.1",
    "@next/bundle-analyzer": "12.1.4",
    "@radix-ui/react-tabs": "0.1.5",
    "@supabase/supabase-js": "^1.33.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "class-variance-authority": "^0.1.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.2",
    "ethers": "^5.6.2",
    "js-confetti": "^0.10.2",
    "lru-cache": "^7.7.3",
    "match-sorter": "^6.3.1",
    "next": "12.1.4",
    "plaiceholder": "^2.3.0",
    "playwright-aws-lambda": "^0.7.0",
    "playwright-core": "^1.20.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-feather": "^2.0.9",
    "react-hot-toast": "^2.2.0",
    "react-markdown": "^8.0.2",
    "react-syntax-highlighter": "^15.5.0",
    "reading-time": "^1.5.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "remark-strip-html": "^1.0.2",
    "simplify-geojson": "^1.0.4",
    "strip-markdown": "^5.0.0",
    "swr": "^1.2.2",
    "tailwindcss": "^3.0.23",
    "wagmi": "^0.2.24",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.0",
    "@types/lru-cache": "^7.6.1",
    "@types/mapbox__polyline": "^1.0.2",
    "@types/react-dom": "^17.0.14",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "^10.4.4",
    "cypress": "^9.5.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.13.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fast-xml-parser": "^4.0.7",
    "feed": "^4.2.2",
    "globby": "^13.1.1",
    "he": "^1.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "node-fetch": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "sharp": "^0.30.3",
    "typescript": "^4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx, json}": [
      "npm run format",
      "npm run lint -- --fix .",
      "git add"
    ]
  }
}
