{
  "name": "timoclasen.de",
  "description": "My personal website.",
  "version": "2.25.0",
  "private": true,
  "author": {
    "name": "Timo Clasen",
    "email": "timo@timoclasen.de",
    "url": "https://timoclasen.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timoclsn/timoclasen.de.git"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@vercel/speed-insights",
      "cypress",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "generate-podcasts": "node ./scripts/podcasts/index.mjs",
    "start": "next start -p 3000",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc",
    "prettier": "prettier --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "format": "pnpm prettier -- --write",
    "check-format": "pnpm prettier -- --list-different",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx ./db/seed.ts",
    "db:reset": "rm ./local.db && pnpm db:push && pnpm db:seed"
  },
  "dependencies": {
    "@effect/platform": "^0.62.1",
    "@formkit/auto-animate": "^0.8.2",
    "@libsql/client": "^0.15.8",
    "@mapbox/polyline": "1.2.1",
    "@radix-ui/react-tabs": "1.1.12",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/typography": "0.5.16",
    "@vercel/speed-insights": "^1.2.0",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "bright": "^1.0.0",
    "cva": "1.0.0-beta.1",
    "date-fns": "3.6.0",
    "date-fns-tz": "3.1.3",
    "drizzle-orm": "^0.44.0",
    "effect": "^3.6.4",
    "lucide-react": "^0.511.0",
    "match-sorter": "6.3.4",
    "next": "15.4.0-canary.56",
    "next-mdx-remote": "^5.0.0",
    "plaiceholder": "3.0.0",
    "react": "19.1.0",
    "react-aria": "^3.40.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "2.5.2",
    "reading-time": "1.5.0",
    "remark": "15.0.1",
    "remark-html": "16.0.1",
    "remark-strip-html": "1.0.2",
    "server-only": "^0.0.1",
    "simplify-geojson": "1.0.5",
    "strip-markdown": "6.0.0",
    "tailwindcss": "4.1.8",
    "tsx": "^4.19.4",
    "use-debounce": "^10.0.4",
    "ws": "8.18.2",
    "zod": "3.25.42",
    "zod-form-data": "^2.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.3.3",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/geojson": "7946.0.16",
    "@types/jest": "29.5.14",
    "@types/mapbox__polyline": "1.0.5",
    "@types/node": "22.15.29",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "@types/ws": "8.18.1",
    "cypress": "14.4.0",
    "dotenv": "16.5.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "9.28.0",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-cypress": "5.0.1",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fast-xml-parser": "5.2.3",
    "feed": "5.1.0",
