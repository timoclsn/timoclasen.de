{
  "name": "timoclasen.de",
  "description": "My personal website.",
  "version": "2.22.0",
  "private": true,
  "author": {
    "name": "Timo Clasen",
    "email": "timo@timoclasen.de",
    "url": "https://timoclasen.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timoclsn/timoclasen.de.git"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "inspect": "NODE_OPTIONS='--inspect' next dev -p 3000",
    "build": "next build",
    "generate-rss": "node ./scripts/generate-rss.mjs",
    "generate-sitemap": "node ./scripts/generate-sitemap.mjs",
    "generate-podcasts": "node ./scripts/podcasts/index.mjs",
    "postbuild": "npm-run-all --parallel generate-rss generate-sitemap",
    "analyze": "ANALYZE=true next build",
    "start": "next start -p 3000",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint --ignore-path .gitignore",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc",
    "prettier": "prettier --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "format": "pnpm prettier -- --write",
    "check-format": "pnpm prettier -- --list-different",
    "validate": "npm-run-all --parallel typecheck check-format lint"
  },
  "dependencies": {
    "@mapbox/polyline": "1.1.1",
    "@next/bundle-analyzer": "13.1.1",
    "@prisma/client": "4.8.1",
    "@radix-ui/react-tabs": "1.0.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "0.5.8",
    "@tanstack/react-query": "4.22.0",
    "@trpc/client": "10.8.1",
    "@trpc/next": "10.8.1",
    "@trpc/react-query": "10.8.1",
    "@trpc/server": "10.8.1",
    "@vercel/og": "0.0.22",
    "class-variance-authority": "0.4.0",
    "clsx": "1.2.1",
    "date-fns": "2.29.3",
    "date-fns-tz": "1.3.7",
    "js-confetti": "0.11.0",
    "match-sorter": "6.3.1",
    "next": "13.1.1",
    "plaiceholder": "2.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "2.0.10",
    "react-hot-toast": "2.4.0",
    "react-markdown": "8.0.4",
    "react-syntax-highlighter": "15.5.0",
    "reading-time": "1.5.0",
    "remark": "14.0.2",
    "remark-html": "15.0.1",
    "remark-strip-html": "1.0.2",
    "simplify-geojson": "1.0.5",
    "strip-markdown": "5.0.0",
    "tailwindcss": "3.2.4",
    "ws": "8.12.0",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/geojson": "7946.0.10",
    "@types/jest": "29.2.5",
    "@types/mapbox__polyline": "1.0.2",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-syntax-highlighter": "15.5.6",
    "@types/testing-library__jest-dom": "5.14.5",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "autoprefixer": "10.4.13",
    "cypress": "12.3.0",
    "dotenv": "16.0.3",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "fast-xml-parser": "4.0.13",
    "feed": "4.2.2",
    "globby": "13.1.3",
    "he": "1.2.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "node-fetch": "3.3.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "prettier": "2.8.2",
    "prettier-plugin-tailwindcss": "0.2.1",
    "prisma": "4.8.1",
    "sharp": "0.31.3",
    "typescript": "4.9.4"
  }
