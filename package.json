{
  "name": "timoclasen.de",
  "description": "My personal website.",
  "version": "2.22.0",
  "private": true,
  "author": {
    "name": "Timo Clasen",
    "email": "timo@timoclasen.de",
    "url": "https://timoclasen.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timoclsn/timoclasen.de.git"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "inspect": "NODE_OPTIONS='--inspect' next dev -p 3000",
    "build": "next build",
    "generate-rss": "node ./scripts/generate-rss.mjs",
    "generate-sitemap": "node ./scripts/generate-sitemap.mjs",
    "generate-podcasts": "node ./scripts/podcasts/index.mjs",
    "postbuild": "npm-run-all --parallel generate-rss generate-sitemap",
    "analyze": "ANALYZE=true next build",
    "start": "next start -p 3000",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint --ignore-path .gitignore",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc",
    "prettier": "prettier --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel typecheck check-format lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mapbox/polyline": "1.1.1",
    "@next/bundle-analyzer": "12.2.0",
    "@radix-ui/react-tabs": "0.1.5",
    "@rainbow-me/rainbowkit": "0.4.1",
    "@supabase/supabase-js": "1.35.4",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/line-clamp": "0.4.0",
    "@tailwindcss/typography": "0.5.2",
    "class-variance-authority": "0.2.2",
    "clsx": "1.1.1",
    "date-fns": "2.28.0",
    "date-fns-tz": "1.3.5",
    "ethers": "5.6.9",
    "js-confetti": "0.10.2",
    "lru-cache": "7.12.0",
    "match-sorter": "6.3.1",
    "next": "12.2.0",
    "plaiceholder": "2.4.0",
    "playwright-aws-lambda": "0.8.0",
    "playwright-core": "1.23.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "2.0.10",
    "react-hot-toast": "2.2.0",
    "react-markdown": "8.0.3",
    "react-syntax-highlighter": "15.5.0",
    "reading-time": "1.5.0",
    "remark": "14.0.2",
    "remark-html": "15.0.1",
    "remark-strip-html": "1.0.2",
    "simplify-geojson": "1.0.5",
    "strip-markdown": "5.0.0",
    "swr": "1.3.0",
    "tailwindcss": "3.1.4",
    "wagmi": "0.5.5",
    "ws": "8.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@types/lru-cache": "7.10.10",
    "@types/mapbox__polyline": "1.0.2",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-syntax-highlighter": "15.5.2",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.4",
    "autoprefixer": "10.4.7",
    "cypress": "10.3.0",
    "dotenv": "16.0.1",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "fast-xml-parser": "4.0.8",
    "feed": "4.2.2",
    "globby": "13.1.2",
    "he": "1.2.0",
    "husky": "8.0.1",
    "jest": "28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "lint-staged": "13.0.3",
    "node-fetch": "3.2.6",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.11",
    "sharp": "0.30.7",
    "typescript": "4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx, json}": [
      "npm run format",
      "npm run lint -- --fix .",
      "git add"
    ]
  }
}
