{
  "name": "timoclasen.de",
  "description": "My personal website.",
  "version": "2.22.0",
  "private": true,
  "author": {
    "name": "Timo Clasen",
    "email": "timo@timoclasen.de",
    "url": "https://timoclasen.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timoclsn/timoclasen.de.git"
  },
  "scripts": {
    "dev": "next dev -p 3000",
    "inspect": "NODE_OPTIONS='--inspect' next dev -p 3000",
    "build": "next build",
    "generate-rss": "node ./scripts/generate-rss.mjs",
    "generate-sitemap": "node ./scripts/generate-sitemap.mjs",
    "generate-podcasts": "node ./scripts/podcasts/index.mjs",
    "postbuild": "npm-run-all --parallel generate-rss generate-sitemap",
    "analyze": "ANALYZE=true next build",
    "start": "next start -p 3000",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint --ignore-path .gitignore",
    "typecheck": "tsc",
    "prettier": "prettier --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.1.1",
    "@next/bundle-analyzer": "12.0.8",
    "@radix-ui/react-tabs": "0.1.4",
    "@supabase/supabase-js": "^1.29.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "ethers": "^5.5.3",
    "js-confetti": "^0.10.2",
    "lru-cache": "^6.0.0",
    "match-sorter": "^6.3.1",
    "next": "12.0.8",
    "plaiceholder": "^2.2.0",
    "playwright-aws-lambda": "^0.7.0",
    "playwright-core": "^1.17.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-hot-toast": "^2.2.0",
    "react-markdown": "^7.1.2",
    "react-syntax-highlighter": "^15.4.5",
    "reading-time": "^1.5.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "remark-strip-html": "^1.0.2",
    "simplify-geojson": "^1.0.4",
    "strip-markdown": "^5.0.0",
    "swr": "^1.1.2",
    "tailwindcss": "^3.0.13",
    "wagmi": "^0.1.3",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@types/lru-cache": "^5.1.1",
    "@types/mapbox__polyline": "^1.0.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "autoprefixer": "^10.4.2",
    "cypress": "^9.2.1",
    "dotenv": "^11.0.0",
    "eslint": "^8.6.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fast-xml-parser": "^4.0.1",
    "feed": "^4.2.2",
    "globby": "^12.0.2",
    "he": "^1.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "node-fetch": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "sharp": "^0.29.3",
    "typescript": "^4.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-types && lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx, json}": [
      "npm run format",
      "npm run lint -- --fix .",
      "git add"
    ]
  }
}
